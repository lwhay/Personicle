USE Personicle;

DROP DATASET BraceletSettings IF EXISTS;

DROP TYPE BraceletSettingsType IF EXISTS;

CREATE TYPE BraceletSettingsType AS OPEN {
  id: UUID,
  netId: int?,
  userName: string?,
  mac: string?,
  unitAndPage0: int,                 -- default (0)
  height: int,                       -- default (180)
  weight: int,                       -- default (700)
  stride: int,                       -- default (0)
  targetType: int,                   -- default (0)
  targetValue: int,                  -- default (10000)
  age: int,                          -- default (20)
  sex: int,                          -- default (0)
  timezone: int,                     -- default (0)
  interval: int,                     -- default (0)
  runType: int,                      -- default (0)
  runTarget: int,                    -- default (0)
  unitAndPage2: int,                 -- default (2)
  perimeter: int,                    -- default (0)
  crank: int,                        -- default (0)
  startHour: int,                    -- default (0)
  startMiniute: int,                 -- default (0)
  mind0: int,                        -- default (0)
  mind1: int,                        -- default (0)
  mind2: int,                        -- default (0)
  reserved2: int,                    -- default (0)
  unitAndPage4: int,                 -- default (0)
  clock0OpenAndRepeat: int,          -- default (0)
  clock0Hour: int,                   -- default (0)
  clock0Minute: int,                 -- default (0)
  clock0Reserved: int,               -- default (0)
  clock1OpenAndRepeat: int,          -- default (0)
  clock1Hour: int,                   -- default (0)
  clock1Minute: int,                 -- default (0)
  clock1Reserved: int,               -- default (0)
  clock2OpenAndRepeat: int,          -- default (0)
  clock2Hour: int,                   -- default (0)
  clock2Minute: int,                 -- default (0)
  clock2Reserved: int,               -- default (0)
  langCode: int,                     -- default (0)
  reserved4: int,                    -- default (0)
  unitAndPage6: int,                 -- default (0)
  openAndRepeat: int,                -- default (0)
  morningHourStart: int,             -- default (0)
  morningHourEnd: int,               -- default (0)
  afternoonHourStart: int,           -- default (0)
  afternoonHourEnd: int,             -- default (0)
  reserved6_0: int,                  -- default (0)
  heartBPSwitch: int,                -- default (0)
  alterHRValue: int,                 -- default (120)
  alterSBPValue: int,                -- default (140)
  alterDBPValue: int,                -- default (90)
  fatigue: int,                      -- default (0)
  reserved6_1: int,                  -- default (0)
  dateString: string?,
  status: string?,
  createTime: bigint?,
  updateTime: bigint?
};

CREATE DATASET BraceletSettings(BraceletSettingsType) PRIMARY KEY id AUTOGENERATED;
CREATE INDEX BraceletSettingsMacIdx ON BraceletSettings(mac) TYPE btree;