USE Personicle;

DROP DATASET BraceletRawList IF EXISTS;

DROP TYPE BraceletRawListType IF EXISTS;

CREATE TYPE BraceletRawListType AS OPEN {
  deviceId: UUID,
  timestamp: bigint,
  netId: int?,
  userName: string?,
  mac: string?,
  tag: string?,
  startTime: bigint?,
  endTime: bigint?,
  count: int,        -- default (0)
  rawDetailUrl: string?,
  dateString: string?,
  status: string?,
  createTime: bigint?,
  updateTime: bigint?
};

CREATE DATASET BraceletRawList(BraceletRawListType) PRIMARY KEY  deviceId, timestamp;
