USE Personicle;

DROP DATASET BraceletHbList IF EXISTS;

DROP TYPE BraceletHbListType IF EXISTS;

CREATE TYPE BraceletHbListType AS OPEN {
  id: UUID,
  netId: int?,
  userName: string?,
  mac: string?,
  dbp: int,          -- default (0)
  sbp: int,          -- default (0)
  heartRate: int,    -- default (0)
  dateString: string?,
  status: string?,
  createTime: bigint?,
  updateTime: bigint?
};

CREATE DATASET BraceletHbList(BraceletHbListType) PRIMARY KEY id AUTOGENERATED;
CREATE INDEX BraceletHbListMacIdx ON BraceletHbList(mac) TYPE btree;