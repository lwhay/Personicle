USE Personicle;

DROP DATASET EmotionVedio IF EXISTS;

DROP TYPE EmotionVedioType IF EXISTS;

CREATE TYPE EmotionVedioType AS OPEN {
  userId: string, --primary key string for user, will be changed to uuid later
  timestamp: bigint, --primary key timestamp for observation
  name: string?,
  path: string,
  comments: string
};

CREATE DATASET EmotionVedio(EmotionVedioType) PRIMARY KEY userId, timestamp;