USE Personicle;

DROP DATASET SensoringGPS IF EXISTS;

DROP TYPE SensoringGPSType IF EXISTS;

CREATE TYPE SensoringGPSType AS OPEN {
  deviceId: string,  --primary key string for user, will be changed to uuid later
  timestamp: bigint, --primary key timestamp for observation
  userName: string?,
  weatherInfo: int,
  latitude: [double],
  longitude: [double],
  comments: string
};

CREATE DATASET SensoringGPS(SensoringGPSType) PRIMARY KEY deviceId, timestamp;
