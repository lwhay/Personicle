USE Personicle;

DROP DATASET BraceletRawDetail IF EXISTS;

DROP TYPE BraceletRawDetailType IF EXISTS;

CREATE TYPE BraceletRawDetailType AS OPEN {
  id: string,
  rawId: string,
  content: string?
};

CREATE DATASET BraceletRawDetail(BraceletRawDetailType) PRIMARY KEY id;
CREATE INDEX BraceletRawDetailRawIdx on BraceletRawDetail(rawId) TYPE btree;